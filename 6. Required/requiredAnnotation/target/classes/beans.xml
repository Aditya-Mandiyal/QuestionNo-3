<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns = "http://www.springframework.org/schema/beans"
   xmlns:xsi = "http://www.w3.org/2001/XMLSchema-instance"
   xmlns:context = "http://www.springframework.org/schema/context"
   xsi:schemaLocation = "http://www.springframework.org/schema/beans
   http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
   http://www.springframework.org/schema/context
   http://www.springframework.org/schema/context/spring-context-3.0.xsd">
   

   <context:annotation-config/>
      
   <bean id="college" class="com.godigit.autowiredannotation.College">
  <!--  <property name="name" value="CGC"></property> -->
   <property name="location" value="Landran"></property>  
   </bean>
   
   <bean id="eceDepartment" class="com.godigit.autowiredannotation.Department">
   <property name="name" value="ECE"></property>
   <property name="hodName" value="dinesh"></property>  
   <property name="noOfTeachers" value="30"></property> 
   </bean>
   
    
  

</beans>